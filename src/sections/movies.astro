---
import { movies } from "../bd/movies";
import { MovieCard } from "../components/react/MovieCard";
---

<section class="flex flex-wrap items-start justify-center md:gap-6">
  {
    movies.map(({ id, releaseDate, title }) => (
      <MovieCard
        id={id}
        title={title}
        realeaseDate={releaseDate}
        client:visible
      />
    ))
  }
</section>
